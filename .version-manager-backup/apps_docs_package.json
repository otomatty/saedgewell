{
  "name": "docs",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "analyze": "ANALYZE=true bun run build",
    "build": "bun run with-env next build",
    "build:test": "NODE_ENV=test bun run with-env:test next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "bun run with-env next dev --turbo --port 7511 | pino-pretty -c",
    "lint": "biome lint .",
    "format": "biome format .",
    "start": "bun run with-env next start",
    "start:test": "NODE_ENV=test bun run with-env:test next start",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ./.env.local --",
    "with-env:test": "dotenv -e ./.env.test --",
    "supabase": "supabase",
    "supabase:start": "supabase status || supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:status": "supabase status",
    "supabase:test": "supabase db test",
    "supabase:db:lint": "supabase db lint",
    "supabase:deploy": "supabase link --project-ref $SUPABASE_PROJECT_REF && supabase db push",
    "supabase:typegen": "bun run supabase:typegen:packages && bun run supabase:typegen:app",
    "supabase:typegen:packages": "supabase gen types typescript --local > ../../packages/supabase/src/database.types.ts",
    "supabase:typegen:app": "supabase gen types typescript --local > ./lib/database.types.ts",
    "supabase:db:dump:local": "supabase db dump --local --data-only",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@edge-csrf/nextjs": "2.5.3-cloudflare-rc1",
    "@google/generative-ai": "0.24.0",
    "@heroicons/react": "2.2.0",
